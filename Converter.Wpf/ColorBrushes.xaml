<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BtnDefault">#F9F8F9</Color>
    <Color x:Key="BtnHover">#6BA2A3</Color>
    <Color x:Key="BtnActive">#BFBFBF</Color>

    <SolidColorBrush x:Key="BtnDefaultBrush" Color="{StaticResource BtnDefault}"></SolidColorBrush>
    <SolidColorBrush x:Key="BtnHoverBrush" Color="{StaticResource BtnHover}"></SolidColorBrush>
    <SolidColorBrush x:Key="BtnActiveBrush" Color="{StaticResource BtnActive}"></SolidColorBrush>
</ResourceDictionary>