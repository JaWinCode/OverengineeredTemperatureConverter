<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Converter.Wpf"  >
    <Color x:Key="BtnDefault">#F9F8F9</Color>
    <Color x:Key="BtnActive">#6BA2A3</Color>

    <SolidColorBrush x:Key="BtnDefaultBrush" Color="{StaticResource BtnDefault}"/>
    <SolidColorBrush x:Key="BtnActiveBrush" Color="{StaticResource BtnActive}"/>

    <BitmapImage x:Key="CloseImage" UriSource="/Images/close.png"/>

    <local:BoolToEnumConverter x:Key="BoolToEnumConverter"/>
    <local:BoolToEnumConverter x:Key="BoolToVisibilityConverter"/>
</ResourceDictionary>